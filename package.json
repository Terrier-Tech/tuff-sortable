{
  "name": "tuff-sortable",
  "version": "0.6.0",
  "type": "module",
  "files": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Terrier-Tech/tuff-sortable"
  },
  "scripts": {
    "dev": "vite",
    "check": "tsc",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepub": "tsc && mkdir -p dist && rm -rf dist/* && cp -a ./src/*.ts ./dist/ && cp ./package.json ./dist/ && cp ./README.md ./dist/",
    "pub": "npm publish ./dist"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "tuff-core": "latest"
  },
  "license": "MIT"
}
